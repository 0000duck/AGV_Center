<Window x:Class="Common_Libraries.Views.SubmitCommand"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Common_Libraries.Views"
        mc:Ignorable="d"
        Title="SubmitCommand" Height="431" Width="800">
    <Grid>
        <Label x:Name="lblSerialNumber" Content="SerialNumber:" HorizontalAlignment="Left" Margin="23,20,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtSerialNumber" HorizontalAlignment="Left" Height="23" Margin="100,22,0,0" TextWrapping="Wrap" Text="AGVXXXXXXXXXXXXXXXX" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btnCancel" Content="Cancel" HorizontalAlignment="Left" Margin="707,372,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="btnOK" Content="OK" HorizontalAlignment="Left" Margin="627,372,0,0" VerticalAlignment="Top" Width="75"/>
    </Grid>
</Window>
