<UserControl x:Class="AGV_Control_Center.Views.CommandServer"
                xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                xmlns:local="clr-namespace:AGV_Control_Center.Views"
                xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
                xmlns:views="clr-namespace:AGV_Control_Center.Views"
                xmlns:prism="http://prismlibrary.com/"
                prism:ViewModelLocator.AutoWireViewModel="True"
                mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Label x:Name="label" Content="Click the start button to start listening Server" HorizontalAlignment="Left" Margin="41,144,0,0" VerticalAlignment="Top" FontSize="12"/>
        <Button x:Name="button_StartServer" Command="{Binding StartServerCommand, UpdateSourceTrigger=PropertyChanged}" Style="{StaticResource SpecialButtonStyle}" HorizontalAlignment="Left" Margin="211,215,0,0" VerticalAlignment="Top" Width="75" Height="75" BorderThickness="0">
            <Image Source="/AGV_Control_Center;component/Resources/Pass.png" Style="{StaticResource ImageOpacity}" ToolTip="Start Server"/>
        </Button>
    </Grid>
</UserControl>
